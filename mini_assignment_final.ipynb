    "class ReviewAspects(BaseModel):\n",
    "    \"\"\"Structured data to extract from a movie review.\"\"\"\n",
    "    \n",
    "    movie_title: Optional[str] = Field(None, description=\"The title of the movie being reviewed, if mentioned.\")\n",
    "    main_topic: str = Field(..., description=\"The single most important subject or aspect of the review (e.g., 'acting quality', 'plot holes', 'soundtrack').\")\n",
    "    sentiment_score: float = Field(..., description=\"A numerical sentiment score between -1.0 (very negative) and 1.0 (very positive) for the review.\")\n",
    "    is_spoiler: bool = Field(..., description=\"True if the review contains a significant plot spoiler, otherwise False.\")\n",
    "    criticism_points: List[str] = Field(..., description=\"A list of up to three specific negative points or criticisms mentioned in the review.\")\n",
    "\n",
    "# Convert the Pydantic model to the JSON Schema format required by the Gemini API\n",
    "response_schema = ReviewAspects.model_json_schema()"
