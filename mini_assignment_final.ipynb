   ],
   "source": [
    "def clean_text(text):\n",
    "    \"\"\"Remove HTML tags, non-alphanumeric characters, collapse spaces, lowercase.\"\"\"\n",
    "    text = re.sub(r'<[^>]*>', ' ', text)           # remove HTML tags (leave a space)\n",
    "    text = re.sub(r'[^0-9a-zA-Z\\s]', ' ', text)    # keep only letters, numbers, spaces\n",
    "    text = text.lower()                             # lowercase\n",
    "    text = re.sub(r'\\s+', ' ', text).strip()        # collapse whitespace\n",
    "    return text\n",
    "\n",
    "# Apply\n",
    "df_sample[\"cleaned_text\"] = df_sample[\"text\"].apply(clean_text)\n",
    "\n",
    "# Display a comparison of raw vs. cleaned text\n",
    "print(\"Raw Text Example:\")\n",
    "print(df_sample[\"text\"].iloc[0])\n",
    "print(\"Cleaned Text Example:\")\n",
    "print(df_sample[\"cleaned_text\"].iloc[0])\n",
    "\n",
    "# Interpretation\n",
    "print(\"Interpretation: The cleaned text removes HTML tags, punctuation and converts everything to lowercase, which simplifies the input for downstream models.\")\n"
   ]
