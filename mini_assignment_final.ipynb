    "def extract_structured_data(review_text: str, schema: dict) -> dict:\n",
    "    \"\"\"Calls the LLM to extract structured data based on a JSON schema.\"\"\"\n",
    "    try:\n",
    "        # Use Gemini 2.5 Flash for the extraction task\n",
    "        completion = client.chat.completions.create(\n",
    "            model=\"gemini-2.5-flash\", # Using Gemini 2.5 Flash via OpenAI-compatible API\n",
    "            messages=[\n",
    "                {\"role\": \"system\", \"content\": \"You are an expert movie review analyst. Extract the requested information from the user's review and return a perfect JSON object that strictly conforms to the provided schema.\"},\n",
    "                {\"role\": \"user\", \"content\": f\"Analyze the following movie review: \\n\\n---\\n\\n{review_text}\"}\n",
    "            ],\n",
    "            response_format={\n",
    "                \"type\": \"json_schema\",\n",
    "                \"json_schema\": {\"name\": \"ReviewAspects\", \"strict\": True, \"schema\": schema}\n",
    "            },\n",
    "            temperature=0.0 # Set low temperature for deterministic extraction\n",
    "        )\n",
    "        \n",
    "        # The response content is the JSON string\n",
    "        json_string = completion.choices[0].message.content\n",
    "        return json.loads(json_string)\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"An error occurred during LLM extraction: {e}\")\n",
    "        return {\"error\": str(e), \"review\": review_text}\n",
    "\n",
    "# Select a small, diverse sample for LLM processing (LLM calls can be slow/costly)\n",
    "llm_sample = df_sample.sample(n=10, random_state=SEED)\n",
    "\n",
    "# Run extraction on the sample\n",
    "extracted_data = []\n",
    "for index, row in tqdm(llm_sample.iterrows(), total=len(llm_sample), desc=\"LLM Extraction\"):\n",
    "    result = extract_structured_data(row['text'], response_schema)\n",
    "    extracted_data.append(result)\n",
    "\n",
    "# Convert results to a DataFrame for easy viewing\n",
    "df_extracted = pd.DataFrame(extracted_data)\n",
    "print(\"\\nStructured Extraction Results:\")\n",
    "print(df_extracted.head())"
   ]
